---
import yourinfo from "../../../YOURINFO.json";
---

<div class="snap-center w-full h-full">
  {
    yourinfo.map(({ name, bio }) => (
      <>
        <h1 class="text-4xl font-bold text-center text-white">{name}</h1>
        <p class="text-2xl text-center text-white">{bio}</p>
      </>
    ))
  }
</div>
